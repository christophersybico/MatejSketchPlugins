{
  "name": "MatejSketchPlugins",
  "description" : "A box for random sketch plugins",
  "author" : "Matej Hrescak",
  "authorEmail" : "matej@hrescak.com",
  "homepage": "http://github.com/hrescak/MatejSketchPlugins",
  "version" : "1.1",
  "identifier" : "com.hrescak.matej-sketch-plugins",
  "compatibleVersion": "3.3",
  "bundleVersion": "1",
  "commands" : [
    {
      "name" : "Align to Pixel",
      "script" : "alignToPixel.js",
      "handler" : "onRun",
      "shortcut" : "cmd shift /'",
      "identifier" : "align-to-pixel"
    },
    {
      "name" : "Select Parent Artboards",
      "script" : "selectParentArtboards.js",
      "handler" : "onRun",
      "shortcut" : "cmd shift a",
      "identifier" : "select-parent-artboards"
    },
    {
      "name" : "Toggle Specs",
      "script" : "toggleSpecs.js",
      "handler" : "onRun",
      "shortcut" : "cmd l",
      "identifier" : "toggle-specs"
    },
    {
      "name" : "Find and Replace in Selected Layer Names",
      "script" : "findReplace.js",
      "handler" : "onRun",
      "identifier" : "find-replace"
    }
  ],
  "menu" : {
    "title" : "Matej Plugins",
    "items" : [
      "toggle-specs",
      "select-parent-artboards",
      "align-to-pixel",
      "find-replace"
    ]
  }
}
